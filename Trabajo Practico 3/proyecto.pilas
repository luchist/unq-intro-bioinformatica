    "actores": [
      {
        "nombre": "actor",
        "codigo": "// @ts-ignore\nclass actor extends Actor {\n\n  toca_el_suelo: boolean;\n  secuencias = \"ACCC\";\n\n  iniciar() {\n    this.estado = \"parado\";\n    this.texto = \"\";\n  }\n\n  actualizar() {\n    let pies = this.obtener_sensor(\"pies\")\n    this.toca_el_suelo = pies.colisiona_con_etiqueta(\"ladrillo\");\n\n    this.pilas.camara.seguir_al_actor(this, 10)\n  }\n\n  public get_secuencias() {\n    return this.secuencias;\n  }\n\n  // Estado: parado\n\n  parado_iniciar() {\n\n  }\n\n  parado_actualizar() {\n    if (this.control.izquierda) {\n      this.estado = \"camina\";\n    }\n\n    if (this.control.derecha) {\n      this.estado = \"camina\";\n    }\n\n    if (this.control.arriba) {\n      this.estado = \"salta\";\n    }\n  }\n\n  // Estado: camina\n  camina_iniciar() {\n  }\n\n  camina_actualizar() {\n    if (this.control.derecha) {\n      this.x += 5;\n      this.espejado = false;\n    }\n\n    if (this.control.izquierda) {\n      this.x -= 5;\n      this.espejado = true;\n    }\n\n    if (this.control.arriba) {\n      this.estado = \"salta\";\n    }\n\n    if (!this.control.derecha && !this.control.izquierda) {\n      this.estado = \"parado\"\n    }\n  }\n\n\n  // Estado: salta\n\n  salta_iniciar() {\n    this.impulsar(0, 7);\n  }\n\n  salta_actualizar() {\n    if (this.control.derecha) {\n      this.x += 5;\n      this.espejado = false;\n    }\n\n    if (this.control.izquierda) {\n      this.x -= 5;\n      this.espejado = true;\n    }\n\n    if (this.toca_el_suelo) {\n      this.estado = \"parado\";\n    }\n  }\n\n\n  cuando_comienza_una_colision(actor: Actor) {\n    if (actor.tiene_etiqueta(\"uracilo\")) {\n      actor.eliminar();\n      this.texto += 'U';\n    }\n\n    if (actor.tiene_etiqueta(\"guanina\")) {\n      actor.eliminar();\n      this.texto += 'G';\n    }\n\n    if (actor.tiene_etiqueta(\"adenina\")) {\n      actor.eliminar();\n      this.texto += 'A';\n    }\n\n    if (actor.tiene_etiqueta(\"citosina\")) {\n      actor.eliminar();\n      this.texto += 'C';\n    }\n\n    if (actor.tiene_etiqueta(\"portal\")) {\n      this.pilas.cambiar_escena(\"nivel2\")\n    }\n  }\n\n}"
